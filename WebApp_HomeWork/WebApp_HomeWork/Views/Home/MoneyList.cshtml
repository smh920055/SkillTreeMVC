@{
    Layout = null;
}
@model List<WebApp_HomeWork.Models.MoneyClass>
@for (int i = 0; i < Model.Count; i++)
{
    <tr>
        <th>@(i + 1)</th>
        <th>@Model[i].category</th>
        <th>@Model[i].date</th>
        <th>@Model[i].money</th>
        <th>@Model[i].description</th>
    </tr>
}

